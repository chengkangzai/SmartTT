{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "algolia/algoliasearch-client-php": "^3.3.2",
        "barryvdh/laravel-translation-manager": "^0.6.4",
        "bezhansalleh/filament-language-switch": "^1.0.6",
        "botman/botman": "^2.8.1",
        "botman/driver-web": "^1.5.3",
        "codeat3/blade-maki-icons": "^1.6",
        "davidhsianturi/blade-bootstrap-icons": "^1.4",
        "doctrine/dbal": "^3.6.1",
        "fakerphp/faker": "^1.21.0",
        "filament/filament": "^2.17.18",
        "filament/spatie-laravel-media-library-plugin": "^2.17.18",
        "filament/spatie-laravel-settings-plugin": "^2.17.18",
        "google/cloud-dialogflow": "^0.27.1",
        "guzzlehttp/guzzle": "^7.5",
        "icetalker/filament-table-repeater": "^1.0.1",
        "jeffgreco13/filament-breezy": "^1.5.2",
        "laravel/cashier": "^13.17",
        "laravel/framework": "^10.5",
        "laravel/scout": "^9.8.1",
        "laravel/tinker": "^2.8.1",
        "laraveldaily/laravel-invoices": "^3.1",
        "league/flysystem-aws-s3-v3": "^3.12.2",
        "league/oauth2-client": "^2.6.1",
        "livewire/livewire": "^2.12.3",
        "microsoft/microsoft-graph": "^1.92",
        "owenvoke/blade-fontawesome": "^2.2",
        "phpsa/filament-authentication": "^2.4",
        "spatie/laravel-activitylog": "^4.7.3",
        "spatie/laravel-livewire-wizard": "^1.1.5",
        "spatie/laravel-medialibrary": "^10.7.15",
        "spatie/laravel-permission": "^5.10.0",
        "spatie/laravel-settings": "^2.8.2",
        "spatie/laravel-stripe-webhooks": "^3.2.3",
        "tanmuhittin/laravel-google-translate": "^2.2",
        "vincechen/botman-studio-dialogflow-v2": "^0.2.0",
        "wire-elements/modal": "^1.0.7"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8.1",
        "barryvdh/laravel-ide-helper": "^2.13",
        "laravel/pint": "^1.7",
        "mockery/mockery": "^1.5.1",
        "nunomaduro/collision": "^7.3.3",
        "pestphp/pest": "^2.3",
        "pestphp/pest-plugin-laravel": "^2.0",
        "pestphp/pest-plugin-livewire": "^2.0",
        "spatie/laravel-ignition": "^2.0",
        "spatie/laravel-remote": "^1.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan filament:upgrade"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "run-ide-helper": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models --no-interaction",
            "@php artisan ide-helper:eloquent"
        ],
        "run-translation": [
            "@php artisan translations:reset",
            "@php artisan translations:import",
            "@php artisan translations:find"
        ],
        "test": [
            "XDEBUG_MODE=off ./vendor/bin/pest --parallel"
        ],
        "test-coverage": [
            "XDEBUG_MODE=coverage ./vendor/bin/pest --coverage --parallel"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
