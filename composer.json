{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-gd": "*",
        "barryvdh/laravel-translation-manager": "^0.6.3",
        "botman/botman": "^2.7",
        "botman/driver-web": "^1.5",
        "fakerphp/faker": "^1.9.1",
        "google/cloud-dialogflow": "^0.27.1",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/cashier": "^13.10",
        "laravel/framework": "^9.2",
        "laravel/sanctum": "^2.14.1",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^3.3",
        "laraveldaily/laravel-invoices": "^3.0",
        "league/flysystem-aws-s3-v3": "^3.0",
        "league/oauth2-client": "^2.6",
        "livewire/livewire": "^2.10",
        "microsoft/microsoft-graph": "^1.67",
        "rappasoft/laravel-livewire-tables": "^2.7",
        "spatie/laravel-activitylog": "^4.5",
        "spatie/laravel-livewire-wizard": "^1.0.1",
        "spatie/laravel-medialibrary": "^10.0.0",
        "spatie/laravel-permission": "^5.5",
        "spatie/laravel-settings": "^2.4",
        "spatie/laravel-stripe-webhooks": "^3.1",
        "tanmuhittin/laravel-google-translate": "^2.1",
        "vincechen/botman-studio-dialogflow-v2": "^0.2.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "barryvdh/laravel-ide-helper": "^2.8",
        "chengkangzai/laravel-stubs": "^2.4",
        "laraveldaily/larastarters": "^1.4",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.2",
        "pestphp/pest-plugin-parallel": "^1.0",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0",
        "spatie/laravel-remote": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "run-ide-helper": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models --no-interaction",
            "@php artisan ide-helper:eloquent"
        ],
        "run-translation": [
            "@php artisan translations:reset",
            "@php artisan translations:import",
            "@php artisan translations:find"
        ],
        "test": [
            "XDEBUG_MODE=off ./vendor/bin/pest --parallel"
        ],
        "test-coverage": [
            "XDEBUG_MODE=coverage ./vendor/bin/pest --coverage --parallel"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
